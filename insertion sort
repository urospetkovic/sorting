import random

num_list = []
for x in range(20):
    num_list.append(random.randint(1, 200))
print(num_list)
def compare(x):  # compares two neighbour numbers
    if num_list[x] > num_list[x+1]:
        num_list[x], num_list[x+1] = num_list[x+1], num_list[x]  # swap places if needed
        if x > 0:
            compare(x-1)  # if not first in list, check one behind 
    return num_list
        
def insertion_sort(num_list):
    for x in range(len(num_list)-1):
        compare(x)
    return num_list
    
        
            
        
insertion_sort(num_list)
print(num_list)
